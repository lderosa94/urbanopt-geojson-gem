{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://json-schema.org/openstudio-urban-modeling/district_system_properties.json#",
  "title": "URBANopt Electrical Wire",
  "description": "Schema for an URBANopt Electrical Wire object",
  "type": "object",
  "properties": {
    "id": {
      "description": "Unique id used to refer to this feature within this dataset.",
      "type": "string"
    },
    "project_id": {
      "description": "Project which this feature belongs to.",
      "type": "string"
    },
    "type": {
      "description": "Type of feature.",
      "type": "string",
      "enum": [
        "ElectricalWire"
      ]
    },
    "source_name": {
      "description": "Name of the original data source.",
      "type": "string"
    },
    "name": {
      "description": "Feature name",
      "type": "string"
    },
    "gmr": {
      "description": "Geometric mean radius of the wire",
      "type": "number"
    },
    "ampacity": {
      "description": "Ampacity of the wire",
      "type": "number"
    },
    "diameter": {
      "description": "Diameter of the wire",
      "type": "number"
    },
    "resistance": {
      "description": "per unit resistance of the wire",
      "type": "number"
    },

    "information_type": {
      "$ref": "#/definitions/ElectricalWireType"
    }
  },
  "required":[
      "type",
      "id",
      "information_type"
  ]
  "additionalProperties": false,
  "definitions": {
    "ElectricalWireType": {
      "description": "Type of electrical wire",
      "type": "string",
      "enum": [
        "Wire"
      ]
    }
  }
}




